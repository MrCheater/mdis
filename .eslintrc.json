{
    "parserOptions": {
        "ecmaVersion": 9,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "indent": ["error", 2, { "SwitchCase": 1 }],
        "semi": ["error", "always"],
        "quotes": ["error", "single"]
    }
}